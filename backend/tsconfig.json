{
  "compilerOptions": {
    
      "target": "ES2020",
      "module": "ESNext",
      "moduleResolution": "bundler",
      "esModuleInterop": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "allowImportingTsExtensions": true,
      "noEmit": true
  }
}
